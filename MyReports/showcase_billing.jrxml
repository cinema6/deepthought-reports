	<variable name="cycle_days_pct" class="java.lang.Double">
		<variableExpression><![CDATA[($F{period_days}.doubleValue() > 0 ? ($F{period_days}.doubleValue() - $F{days_remaining}.doubleValue()) / $F{period_days}.doubleValue(): 0)]]></variableExpression>
	</variable>
	<variable name="cycle_users_pct" class="java.lang.Double">
		<variableExpression><![CDATA[((Number)(IF($F{view_target} > 0,$F{view_target},$F{cycle_user_views}))).doubleValue() > 0 ? $F{cycle_user_views}.doubleValue() / ((Number)(IF($F{view_target} > 0,$F{view_target},$F{cycle_user_views}))).doubleValue(): 0]]></variableExpression>
	</variable>
	<variable name="avg_userview_rate_lifetime" class="java.lang.Double">
		<variableExpression><![CDATA[IF((double)$F{impressions} > 0,(double)$F{user_views} / (double)$F{impressions},0)]]></variableExpression>
	</variable>
	<variable name="avg_userview_rate_three_day" class="java.lang.Double">
		<variableExpression><![CDATA[IF((double)$F{avg_impressions_three_day} > 0,(double)$F{avg_users_three_day}/ (double)$F{avg_impressions_three_day} ,0)]]></variableExpression>
	</variable>
	<variable name="cycle_imps_pct" class="java.lang.Double">
		<variableExpression><![CDATA[$F{est_impressions}.doubleValue() > 0 ? $F{cycle_impressions}.doubleValue() / $F{est_impressions}.doubleValue(): 0]]></variableExpression>
	</variable>
	<variable name="campaign_warnings" class="java.lang.String">
		<variableExpression><![CDATA[($F{days_remaining} < 0) ? "" : ((($V{cycle_days_pct} > 0.5) && ($V{cycle_days_pct} - $V{cycle_users_pct}) >= 0.05) ? "U" : "") + ($V{cycle_imps_pct} > 0.80 ? "I" : "")]]></variableExpression>
	</variable>
	<variable name="cycle_hours_pct" class="java.lang.Double">
		<variableExpression><![CDATA[($F{period_hours}.doubleValue() > 0 ? ($F{period_hours}.doubleValue() - $F{hours_remaining}.doubleValue()) / $F{period_hours}.doubleValue(): 0)]]></variableExpression>
	</variable>
	<variable name="record_count" class="java.lang.Integer" calculation="Count">
		<variableExpression><![CDATA[$F{campaign_id}]]></variableExpression>
	</variable>
	<variable name="avg_click_rate_three_day" class="java.lang.Double">
		<variableExpression><![CDATA[IF((double)$F{avg_views_three_day} > 0,(double)$F{avg_clicks_three_day}/ (double)$F{avg_views_three_day} ,0)]]></variableExpression>
	</variable>
