	</parameter>
	<queryString>
		<![CDATA[select initcap(u.fname) || ' ' || initcap(u.lname) as "user", u.email, coalesce(u.src_doc->>'company',o.name) as company ,
(u.created_ts at time zone 'America/New_York')::date as signed_up_date, 
case u.status when 'new' then 'Unconfirmed' when 'active' then 'Activated' else u.status end as status, 
c.campaign_id,
c.name as campaign,
p.src_doc->>'thumbnail' as campaign_logo_url,
c.status as campaign_status,
case coalesce(o.src_doc->>'braintreeCustomer','') when '' then false else true end as has_payment_method,
c.updated_ts as campaign_status_time,
coalesce(a.src_doc->'beeswaxIds'->>'advertiser','') as beeswax_advertiser_id,
coalesce(c.src_doc->'externalCampaigns'->'beeswax'->>'externalId','') as beeswax_campaign_id,
c.src_doc->'product'->>'uri' as store_uri,
obs.user_views as cycle_user_views,
obs.billing_start,
obs.billing_end,
obs.period_days,
obs.view_target,
obs.days_remaining,
(obs.period_days * 24)::integer as period_hours,
(EXTRACT(EPOCH FROM (obs.billing_end + '1 day'::interval)::date - date_trunc('hour',current_timestamp))/3600)::integer as hours_remaining,
obs.est_impressions,
coalesce(ci.cycle_impressions,0)::integer as cycle_impressions,
coalesce(ai.avg_impressions_three_day,0)::integer as avg_impressions_three_day,
coalesce(au.avg_users_three_day,0)::integer as avg_users_three_day,
coalesce(ac.avg_clicks_three_day,0)::integer as avg_clicks_three_day,
coalesce(acv.avg_views_three_day,0)::integer as avg_views_three_day,
coalesce(uu.unique_user_views,0)::integer as user_views,
coalesce(i.impressions,0)::integer as impressions,
coalesce(cv.views,0)::integer as views,
coalesce(cc.clicks,0)::integer as clicks,
case when coalesce(ai.avg_impressions_three_day,0) > 0 then coalesce(au.avg_users_three_day,0)::float/ ai.avg_impressions_three_day::float else 0::float end as avg_userview_rate_3_day,
case when coalesce(i.impressions,0) > 0 then coalesce(uu.unique_user_views,0)::float/ i.impressions::float else 0::float end as avg_userview_rate_lifetime
from dim.user u
left join dim.org o on u.org_id = o.org_id AND o.end_ts = dim.sp_end_of_time()
left join dim.v_org_showcase_billing_status obs ON u.org_id = obs.org_id
left join dim.v_user_role ur ON u.user_id = ur.user_id
left join dim.campaign c on c.user_id = u.user_id AND c.end_ts = dim.sp_end_of_time() --and c.status = 'active'
left join dim.placement p on c.campaign_id = p.campaign_id  and p.src_doc->>'tagType' = 'mraid' and p.end_ts = dim.sp_end_of_time()
left join dim.advertiser a ON c.advertiser_id = a.advertiser_id AND  a.end_ts = dim.sp_end_of_time()
left join (
   select campaign_id,(sum(events) / 3)::integer as avg_impressions_three_day
   from rpt.campaign_summary_hourly
   where event_type = 'impression'
       and rec_ts::date > (current_timestamp::date - '4 day'::interval) and rec_ts < current_timestamp::date
   group by 1
) ai ON ai.campaign_id = c.campaign_id
left join (
   select campaign_id,(count(*) / 3)::integer as avg_users_three_day
   from (
      select campaign_id,uuid
      from fct.showcase_user_views_daily
      where rec_date > (current_timestamp::date - '4 day'::interval) and rec_date < current_timestamp::date
      group by 1,2
   ) f
   group by 1
) au on au.campaign_id = c.campaign_id
left join (
   select campaign_id,(sum(events) / 3)::integer as avg_clicks_three_day
   from rpt.campaign_summary_hourly
   where event_type = 'link.Action'
       and rec_ts::date > (current_timestamp::date - '4 day'::interval) and rec_ts < current_timestamp::date
   group by 1
) ac ON ac.campaign_id = c.campaign_id
left join (
   select campaign_id,(sum(events) / 3)::integer as avg_views_three_day
   from rpt.campaign_summary_hourly
   where event_type = 'completedView'
       and rec_ts::date > (current_timestamp::date - '4 day'::interval) and rec_ts < current_timestamp::date
   group by 1
) acv ON acv.campaign_id = c.campaign_id
left join rpt.unique_user_views uu on uu.campaign_id = c.campaign_id
 left join (
	select campaign_id,sum(events) as impressions
	from rpt.campaign_summary_hourly
	where event_type = 'impression'
	group by 1
) i on i.campaign_id = c.campaign_id
left join (
	select campaign_id,sum(events) as views
	from rpt.campaign_summary_hourly
	where event_type = 'completedView'
	group by 1
) cv on cv.campaign_id = c.campaign_id
left join (
	select campaign_id,sum(events) as clicks
	from rpt.campaign_summary_hourly
	where event_type = 'link.Action'
	group by 1
) cc on cc.campaign_id = c.campaign_id
left join (
   select sh.campaign_id,sum(events) as cycle_impressions
   from rpt.campaign_summary_hourly sh
   INNER JOIN (
	 SELECT campaign_id,billing_start
	 from  (
		SELECT DISTINCT c.campaign_id,c.org_id
		FROM dim.campaign c
		WHERE c.application = 'showcase' AND c.end_ts = dim.sp_end_of_time()
	) co
	JOIN ( 
		SELECT billing_transactions.org_id,
		max(billing_transactions.transaction_ts)::date AS billing_start
		FROM fct.billing_transactions
		WHERE billing_transactions.sign = 1
		GROUP BY billing_transactions.org_id
	  ) bs ON co.org_id = bs.org_id
    ) cbs ON sh.campaign_id = cbs.campaign_id AND sh.rec_ts >= cbs.billing_start and sh.event_type = 'impression'
    group by 1	
) ci on ci.campaign_id = c.campaign_id
where u.end_ts = dim.sp_end_of_time() and u.self_serve = 't' and ur.system_role = 'showcaseUser'
and u.status <> 'deleted'
AND not u.email in ('minznerj@kean.edu', 'jason@cinema6.com','info@mediasolu.com')
AND NOT c.campaign_id is null
AND NOT c.status in ('canceled','deleted')
AND obs.days_remaining >= $P{days_back} AND obs.days_remaining <= $P{days_forward}
order by obs.billing_end]]>
	</queryString>
