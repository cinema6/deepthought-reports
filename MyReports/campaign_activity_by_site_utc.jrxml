	<parameter name="start_date" class="java.util.Date">
		<defaultValueExpression><![CDATA[DATE(  YEAR(TODAY() ), MONTH(TODAY()), 1)]]></defaultValueExpression>
	</parameter>
	<parameter name="end_date" class="java.util.Date">
		<defaultValueExpression><![CDATA[TODAY( )]]></defaultValueExpression>
	</parameter>
	<parameter name="site_list" class="java.util.List">
		<parameterDescription><![CDATA[List of sites.]]></parameterDescription>
	</parameter>
	<queryString language="SQL">
		<![CDATA[select i.site_name,c.campaign,sum(i.crd_count) as cards, sum(i.cardviews) as cardviews, sum(i.plays) as plays, sum(i.adcounts) as adcounts, sum(spend_est) as spend_est
from (
	select campaign_id,card_id,site_name,sum(cardviews) as cardviews, sum(play) as plays, sum(adcount) as adcounts, sum(spend_est) as spend_est, 1 as crd_count
	from (
		select a.campaign_id,a.card_id,s.site_name,a.cardviews, a.play,a.adcount,a.adcount * cp.cost_per as spend_est
		from rpt.sponsored_card_activity a
		LEFT JOIN dim.campaign_pricing cp ON a.campaign_id = cp.campaign_id AND a.rec_ts between cp.start_ts and cp.end_ts
		LEFT join rpt.hostname_to_site s ON a.hostname = s.hostname AND a.container = s.container
		where  a.rec_ts::date BETWEEN $P{start_date} AND $P{end_date} and $X{IN, site_name, site_list} and s.site_name is not null
	) f
	group by campaign_id,card_id,site_name
) i
inner join dim.v_sponsored_card c ON i.campaign_id = c.campaign_id AND i.card_id = c.card_id
group by i.site_name,c.campaign
order by i.site_name,c.campaign;]]>
	</queryString>
