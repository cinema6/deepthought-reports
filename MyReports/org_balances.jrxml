	<subDataset name="debit_credit_totals" uuid="9dc72df0-e561-4695-86fa-7c7b2b6b8c70">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="deepthought"/>
		<parameter name="email" class="java.lang.String"/>
		<queryString>
			<![CDATA[
select sign,sum(amount) as amount
from (
	select s.sign, s.amount
	from (VALUES(1,0::numeric),(-1,0::numeric)) as s(sign,amount)
	UNION
	select b.sign,sum(b.amount) as amount
	from dim.user u
	inner join dim.org o ON u.org_id = o.org_id AND o.end_ts = dim.sp_end_of_time()
	inner join fct.billing_transactions b on o.org_id = b.org_id
	left join dim.promotion p ON b.promotion_id = p.promo_id AND p.end_ts = dim.sp_end_of_time()
	where u.end_ts = dim.sp_end_of_time() and u.email = $P{email}
	group by 1
) q
group by 1
order by 1 desc]]>
		</queryString>
		<field name="sign" class="java.lang.Integer"/>
		<field name="amount" class="java.math.BigDecimal"/>
		<variable name="balance" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{amount}.multiply(new BigDecimal($F{sign}))]]></variableExpression>
		</variable>
	</subDataset>
	<subDataset name="campaign_counts_by_status" uuid="6a86eee7-225e-4ea5-8854-d0db83d0521c">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="deepthought"/>
		<parameter name="email" class="java.lang.String"/>
		<queryString>
			<![CDATA[select cs.status_name,coalesce(a.campaigns,0) as campaigns, '' as const
from (
	select status, case status when 'outOfBudget' then 'Out of Budget' else initcap(status) end as status_name from dim.campaign group by 1,2 order by 1
) cs
left join
(
	select c.status,count(*) as campaigns
	from dim.user u
	inner join dim.org o ON u.org_id = o.org_id AND o.end_ts = dim.sp_end_of_time()
	inner join dim.campaign c ON o.org_id = c.org_id and c.end_ts = dim.sp_end_of_time()
	where u.end_ts = dim.sp_end_of_time() and u.email = $P{email}
	group by 1
) a
ON cs.status = a.status
order by 1 ]]>
		</queryString>
		<field name="status_name" class="java.lang.String"/>
		<field name="campaigns" class="java.lang.Long"/>
		<field name="const" class="java.lang.String"/>
	</subDataset>
	<subDataset name="most_recent_campaigns" uuid="ffd4111d-672d-49e1-ba59-3c9777cf2a7a">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="deepthought"/>
		<parameter name="email" class="java.lang.String"/>
		<queryString>
			<![CDATA[select c.campaign_id,c.name as campaign,c.created_ts at time zone 'America/New_York' as created, c.updated_ts at time zone 'America/New_York' as last_updated, cp.budget, cp.daily_limit, 
b.amount as spend, b.units as paid_views,bf.units as unpaid_views,
case c.status when 'outOfBudget' then 'Out of Budget' else initcap(c.status) end as status_name
from dim.user u
inner join dim.org o ON u.org_id = o.org_id AND o.end_ts = dim.sp_end_of_time()
inner join dim.campaign c ON u.org_id = c.org_id AND c.end_ts = dim.sp_end_of_time()
left join dim.campaign_pricing cp ON cp.campaign_id = c.campaign_id and cp.end_ts = dim.sp_end_of_time()
left join (
	select org_id,campaign_id,sum(amount) as amount, sum(units) as units
	from fct.billing_transactions
	where sign = -1 and amount > 0
	group by 1,2
) b ON b.org_id = o.org_id AND b.campaign_id = c.campaign_id
left join (
	select org_id,campaign_id,sum(amount) as amount, sum(units) as units
	from fct.billing_transactions
	where sign = -1 and amount = 0
	group by 1,2
) bf ON bf.org_id = o.org_id AND bf.campaign_id = c.campaign_id
where u.end_ts = dim.sp_end_of_time() and u.email = $P{email} and c.status <> 'deleted'
order by 3 desc
limit 5;]]>
		</queryString>
		<field name="campaign_id" class="java.lang.String"/>
		<field name="campaign" class="java.lang.String"/>
		<field name="created" class="java.sql.Timestamp"/>
		<field name="last_updated" class="java.sql.Timestamp"/>
		<field name="budget" class="java.math.BigDecimal"/>
		<field name="daily_limit" class="java.math.BigDecimal"/>
		<field name="spend" class="java.math.BigDecimal"/>
		<field name="paid_views" class="java.lang.Long"/>
		<field name="unpaid_views" class="java.lang.Long"/>
		<field name="status_name" class="java.lang.String"/>
	</subDataset>
	<parameter name="select_billable_email_address" class="java.lang.String">
		<parameterDescription><![CDATA[]]></parameterDescription>
	</parameter>
	<queryString>
		<![CDATA[select coalesce(u.company,o.name) as company,u.fname || ' ' || u.lname as contact_name,u.email as contact_email ,b.transaction_ts at time zone 'America/New_York' as transaction_ts,
b.amount,case when not b.braintree_id is null then 'Braintree Deposit' else coalesce(p.name,b.promotion_id) end as source,
coalesce(r.name,'') as referral_name, coalesce(r.code,'') as referral_code
from dim.user u
inner join dim.org o ON u.org_id = o.org_id AND o.end_ts = dim.sp_end_of_time()
inner join fct.billing_transactions b on o.org_id = b.org_id
left join dim.promotion p ON b.promotion_id = p.promo_id AND p.end_ts = dim.sp_end_of_time()
left join dim.referral_code r ON o.referral_code = r.code AND r.end_ts = dim.sp_end_of_time()
where u.end_ts = dim.sp_end_of_time() and b.sign = 1 and u.email = $P{select_billable_email_address}
order by b.transaction_ts]]>
	</queryString>
	<field name="company" class="java.lang.String"/>
	<field name="contact_name" class="java.lang.String"/>
	<field name="contact_email" class="java.lang.String"/>
	<field name="transaction_ts" class="java.sql.Timestamp"/>
	<field name="amount" class="java.math.BigDecimal"/>
	<field name="source" class="java.lang.String"/>
	<field name="referral_name" class="java.lang.String"/>
	<field name="referral_code" class="java.lang.String"/>
